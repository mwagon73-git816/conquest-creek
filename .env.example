# Firebase Configuration
VITE_FIREBASE_API_KEY=your_firebase_api_key_here
VITE_FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your-project-id
VITE_FIREBASE_STORAGE_BUCKET=your-project.firebasestorage.app
VITE_FIREBASE_MESSAGING_SENDER_ID=your_messaging_sender_id
VITE_FIREBASE_APP_ID=your_app_id

# SMS Feature Flag
# Set to 'true' to enable SMS notifications (requires Twilio configuration)
# Set to 'false' to disable SMS notifications and hide SMS-related UI elements
# Note: SMS feature is currently disabled pending regulatory approval
VITE_SMS_ENABLED=true

# Twilio Configuration (for SMS notifications)
TWILIO_ACCOUNT_SID=your_twilio_account_sid_here
TWILIO_AUTH_TOKEN=your_twilio_auth_token_here
TWILIO_PHONE_NUMBER=your_twilio_phone_number_here
TEST_PHONE_NUMBER=your_test_phone_number_here
MESSAGE_SID=message_sid_for_status_checks
